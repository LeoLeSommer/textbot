{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Android Debug (Launch)",
      "type": "android",
      "request": "launch",
      "processId": "com.example.textbot",
      "preLaunchTask": "Install Debug"
    },
    {
      "name": "Android Debug (Launch) 2",
      "type": "android",
      "request": "launch",
      "preLaunchTask": "Install Debug",
      "apkFile": "${workspaceFolder}/app/build/outputs/apk/debug/app-debug.apk",
      "appSrcRoot": "${workspaceFolder}/app/src/main/java"
    },
    {
      "name": "Attach to Android Process",
      "type": "android",
      "request": "attach",
      "processId": "com.example.textbot",
      "appSrcRoot": "${workspaceFolder}/app/src/main/java"
    },
    {
      "name": "Java Attach (JDWP)",
      "type": "java",
      "request": "attach",
      "hostName": "localhost",
      "port": 5005
    }
  ]
}
